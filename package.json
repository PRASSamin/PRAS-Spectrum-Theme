{
  "name": "nykro-theme",
  "displayName": "Nykro Theme",
  "description": "Nykro Theme is vibrant and focused theme for VS code. with bold colors and clear contrasts, it enhances readability and productivity, making coding a pleasure",
  "version": "0.4.0",
  "publisher": "PRAS",
  "scripts": {
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PRASSamin/nykro-theme.git"
  },
  "icon": "nykro.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "keywords": [
    "color-theme",
    "dark-theme",
    "nykro",
    "pras",
    "prassamin",
    "nykro-theme",
    "theme"
  ],
  "preview": true,
  "galleryBanner": {
    "color": "#0B0E14",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "uiTheme": "vs-dark",
        "label": "Nykro Abyss",
        "path": "./themes/color-theme/nykro-dark-abyss.json"
      },
      {
        "uiTheme": "vs-dark",
        "label": "Nykro Twilight Slate",
        "path": "./themes/color-theme/nykro-twilight-slate.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "nykro-product-icons",
        "label": "Nykro Product Icon",
        "path": "./themes/product-icon-theme/nykro-product-icon.json"
      }
    ]
  },
  "devDependencies": {
    "open": "^7.0.3",
    "webfont": "^11.2.4"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
