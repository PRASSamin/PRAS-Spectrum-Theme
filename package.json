{
	"name": "pras-spectrum-theme",
	"displayName": "PRAS Spectrum Theme",
	"description": "PRAS Spectrum Theme is vibrant and focused theme for VS code. with bold colors and clear contrasts, it enhances readability and productivity, making coding a pleasure",
	"version": "0.3.0",
	"publisher": "PRASSamin",
	"repository": {
		"type": "git",
		"url": "https://github.com/PRASSamin/PRAS-Spectrum-Theme.git"
	},
	"icon": "img/logo.jpg",
	"engines": {
		"vscode": "^1.86.0"
	},
	"keywords": [
		"color-theme",
		"dark-theme",
		"pras",
		"theme",
		"icon-theme"
	],
	"preview": true,
	"galleryBanner": {
		"color": "#000000",
		"theme": "dark"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"uiTheme": "vs-dark",
				"label": "PRAS Dark Abyss",
				"path": "./themes/color-theme/pras-dark-theme.json"
			},
			{
				"uiTheme": "vs-dark",
				"label": "PRAS Twilight Slate",
				"path": "./themes/color-theme/pras-twilight-slate.json"
			}
		],
		"iconThemes": [
			{
				"id": "pras-icon",
				"label": "PRAS Icon",
				"path": "./themes/icon-theme/pras-icons.json"
			}
		],
		"productIconThemes": [
			{
				"id": "pras-product-icons",
				"label": "PRAS Product Icon",
				"path": "./themes/product-icon-theme/pras-product-icon.json"
			}
		]
	},
	"devDependencies": {
		"open": "^7.0.3",
		"webfont": "^11.2.4"
